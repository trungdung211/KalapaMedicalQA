        "import pandas as pd\n",
        "df = pd.read_csv(\"./data/public_test_with_context_en.csv\")"
      ],
      "metadata": {
        "id": "_ZRhWO1baCMf"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "convert_to_binary = {\n",
        "    0: '10000',\n",
        "    1: '01000',\n",
        "    2: '00100',\n",
        "    3: '00010',\n",
        "    4: '00001'\n",
        "}\n",
        "\n",
        "def predict_per_sample(df, model, tokenizer):\n",
        "    answers = []\n",
        "    for idx, row in df.iterrows():\n",
        "        context = row[\"context\"].split(\"///\")[0]\n",
        "        question = row[\"question\"]\n",
        "        choices = [\n",
        "            op for op in row[[\"option_1\", \"option_2\", \"option_3\", \"option_4\", \"option_5\", \"option_6\"]].tolist()\n",
        "            if isinstance(op, str)\n",
        "        ]\n",
        "        text_choices = \" \".join(choices)\n",
        "        prompt = f\"Context: {context}. Question: {question}. Choice the correct answers from: {text_choices}\"\n",
        "        model_inputs = tokenizer(prompt, return_tensors=\"pt\")\n",
        "        outputs = model(**model_inputs)\n",
        "        prediction = torch.argmax(outputs[0], axis=1)\n",
        "        answer = convert_to_binary[prediction.item()]\n",
        "        answer = answer[:len(choices)]\n",
        "        answers.append(answer)\n",
        "    return answers"
      ],
      "metadata": {
        "id": "jsMxPykpaFUj"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model = model.to(\"cpu\")"
      ],
      "metadata": {
        "id": "agC4hp4KaHsi"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "answers = predict_per_sample(df, model, tokenizer)\n",
        "answers"
